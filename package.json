{
  "name": "@magiclab/vrt-runner",
  "version": "0.0.12",
  "description": "VRT runner and result generator for images",
  "main": "build/index.js",
  "repository": "https://github.com/badoo/vrt-runner",
  "author": "Nikhil Verma <nikhil.verma@magiclab.co>",
  "license": "MIT",
  "private": false,
  "bin": "build/bin.js",
  "keywords": [
    "vrt",
    "vrt-runner"
  ],
  "devDependencies": {
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/eslint": "^6.8.1",
    "@types/fs-extra": "^9.0.0",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^10.17.21",
    "@types/prettier": "^2.0.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint": "^7.0.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "start": "HELLO",
    "lint": "npm run tsc:lint && eslint . --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "build": "rm -rf build && mkdir -p build && cp -R src/report-assets build && ./node_modules/.bin/tsc",
    "styles": "npx node-sass --no-source-map ./src/report-assets/css/vrt.scss ./src/report-assets/css/vrt.css --output-style compressed",
    "tsc:lint": "tsc --incremental --noEmit false",
    "test:clean": "rm -rf tmp/result",
    "test": "npm run build && npm run test:clean && node build/bin.js --cwd test/1 --output tmp/result",
    "test:debug": "npm run build && node --inspect --inspect-brk build/bin.js --cwd test/1/ --output tmp/result/",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "execa": "^4.0.3",
    "fs-extra": "^9.0.0",
    "mkdirp": "^1.0.4",
    "reg-cli": "^0.16.1",
    "yargs": "^15.3.1"
  }
}
